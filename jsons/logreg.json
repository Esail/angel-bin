{
  "data": {
    "format": "dummy",
    "indexrange": 148,
    "validateratio": 0.1,
    "sampleratio": 0.1
  },

  "train": {
    "epoch": 100,
    "lr": 0.02
  },

  "model": {
    "modeltype": "T_FLOAT_DENSE"
  },

  "default_optimizer": {
    "type": "adam",
    "reg2": 0.01
  },

  "layers": [
    {
      "name": "input",
      "type": "sparseinputlayer",
      "outputdim": 1,
      "transfunc": "identity"
    },
    {
      "name": "simplelosslayer",
      "type": "simplelosslayer",
      "lossfunc": "logloss",
      "inputlayer": "input"
    }
  ]
}
