{
  "data": {
    "format": "dummy",
    "indexrange": 148,
    "validateratio": 0.1,
    "numfield": 13,
    "sampleratio": 0.1
  },

  "train": {
    "epoch": 100,
    "lr": 0.02,
    "decay":0.0
  },

  "model": {
    "modeltype": "T_FLOAT_DENSE"
  },

  "default_optimizer": {
    "type": "adam",
    "reg2": 0.01
  },

  "layers": [
    {
      "name": "first",
      "type": "sparseinputlayer",
      "outputdim": 1,
      "transfunc": "identity"
    },
    {
      "name": "embedding",
      "type": "embedding",
      "numfactors": 5,
      "outputdim": 65
    },
    {
      "name": "second",
      "type": "BiInnerSumCross",
      "inputlayer": "embedding",
      "outputdim": 1
    },
    {
      "name": "sum",
      "type": "SumPooling",
      "outputdim": 1,
      "inputlayers": [
        "first",
        "second"
      ]
    },
    {
      "name": "simplelosslayer",
      "type": "simplelosslayer",
      "lossfunc": "logloss",
      "inputlayer": "sum"
    }
  ]
}
